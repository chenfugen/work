.container {
    font-size: 32rpx;
    min-height: 100vh;
	width: 100vw;
    overflow: auto;
    color: #333;
}

.marginTop {
    margin-top: 20rpx;
}

.marginBottom {
    margin-bottom: 20rpx;
}

.marginLeft {
    margin-left: 20rpx;
}

.marginRight {
    margin-right: 20rpx;
}

button::after {
    border: none;
}

.clearfloat {
    zoom: 1;
}

.clearfloat:after {
    display: block;
    visibility: hidden;
    clear: both;
    height: 0;
    content: "";
}

.left {
    float: left;
}

.right {
    float: right;
}

.warning {
    color: #f56c6c;
}

.primary {
    color: #409eff;
}

.success {
    color: #67c23a;
}

.placeholderStyle {
    font-size: 32rpx;
    color: #ccc;
    line-height: 40rpx;
}
